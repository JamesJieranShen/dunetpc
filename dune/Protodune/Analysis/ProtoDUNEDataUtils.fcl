
BEGIN_PROLOG

standard_protodunedatautils:
{
   TimingTag:               "timingrawdecoder:daq"  
   BeamEventTag:            "beamevent"

   StrictTOF:               true # strict/permissive on events with invalid TOF data
   StrictCherenkov:         true # strict/permissive on events with invalid Cherenkov data

   TOFDistance:             28.6 # meters
   MomentumScaleFactor:     1.
   MomentumOffset:          0. # GeV/c
   TOFScaleFactor:          1.
   TOFOffset:               59.6 # ns

   ## TOF Cuts for GetTOFParticleID [min,max]
   TOFElectronCuts:         [-1e3,1e3]
   TOFMuonCuts:             [-1e3,160]
   TOFPionCuts:             [-1e3,160]
   TOFKaonCuts:             [-1e3,1e3]
   TOFProtonCuts:           [160,1e3]

}

standard_protodunedatautils_0p3GeV:                 @local::standard_protodunedatautils
standard_protodunedatautils_0p3GeV.TOFElectronCuts: [-1e3,1e3]
standard_protodunedatautils_0p3GeV.TOFMuonCuts:     [-1e3,170]
